{
  "name": "linqua",
  "version": "1.3.11",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "./scripts/start-dev-with-test-data.sh",
    "start:ng:development": "ng serve --configuration development",
    "start:ng:production": "ng serve --configuration production",
    "start:prod": "http-server dist/wwwroot -p 8080 --proxy https://linqua.app",
    "start:prod:e2e": "run-p --race start:prod e2e:watch",
    "start:firebase-emulators:only-functions": "firebase emulators:start --only functions",
    "start:firebase-emulators:backend": "firebase emulators:start --only functions,auth,firestore --inspect-functions 9229",
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "test:watch": "ng test",
    "ng-test": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "test": "firebase emulators:exec --only functions,auth,firestore './test.sh'",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "playwright test",
    "e2e:watch": "playwright test --ui",
    "e2e:ng": "ng e2e",
    "e2e:ng:prod": "ng e2e --configuration production",
    "e2e:ci": "firebase emulators:exec --log-verbosity SILENT --only functions,auth,firestore './e2e-ci.sh'",
    "http-server": "http-server",
    "npm-run-all": "npm-run-all",
    "ci": "run-s lint test e2e:ci build:prod",
    "deploy:firebase": "firebase deploy",
    "deploy:firebase:hosting": "firebase deploy --only hosting",
    "deploy": "run-s ci deploy:firebase",
    "deploy:only-frontend": "run-s ci deploy:firebase:hosting",
    "deploy:only-frontend:skip-ci": "run-s build:prod deploy:firebase:hosting",
    "format": "npx prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/cdk": "~20.2.8",
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/material": "~20.2.8",
    "@angular/platform-browser": "^20.3.4",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@angular/service-worker": "^20.3.4",
    "firebase": "^12.4.0",
    "immer": "~10.1.3",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@playwright/test": "~1.56.0",
    "@types/jasmine": "~5.1.9",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^22.18.9",
    "angular-eslint": "^20.3.0",
    "eslint": "~9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "~4.2.0",
    "firebase-admin": "~13.5.0",
    "http-server": "^14.1.0",
    "jasmine-core": "~5.12.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "playwright-ng-schematics": "^2.1.1",
    "prettier": "~3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.46.0"
  }
}
