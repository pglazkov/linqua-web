{
  "name": "linqua",
  "version": "1.2.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-p --race start:firebase-emulators:backend start:ng:development",
    "start:ng:development": "ng serve --configuration development",
    "start:ng:production": "ng serve --configuration production",
    "start:prod": "http-server dist/wwwroot -p 8080 --proxy https://linqua-app.com",
    "start:prod:e2e": "run-p --race start:prod e2e:watch",
    "start:firebase-emulators:only-functions": "firebase emulators:start --only functions",
    "start:firebase-emulators:backend": "firebase emulators:start --only functions,auth,firestore --inspect-functions 9229",
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "test:watch": "ng test",
    "ng-test": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "test": "firebase emulators:exec --only functions,auth,firestore './test.sh'",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "playwright test",
    "e2e:watch": "playwright test --ui",
    "e2e:ng": "ng e2e",
    "e2e:ng:prod": "ng e2e --configuration production",
    "e2e:ci": "firebase emulators:exec --log-verbosity SILENT --only functions,auth,firestore './e2e-ci.sh'",
    "http-server": "http-server",
    "npm-run-all": "npm-run-all",
    "ci": "run-s lint test e2e:ci build:prod",
    "deploy:firebase": "firebase deploy",
    "deploy:firebase:hosting": "firebase deploy --only hosting",
    "deploy": "run-s ci deploy:firebase",
    "deploy:only-frontend": "run-s ci deploy:firebase:hosting",
    "deploy:only-frontend:skip-ci": "run-s build:prod deploy:firebase:hosting",
    "format": "npx prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/material": "^19.0.1",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/router": "^19.0.1",
    "@angular/service-worker": "^19.0.1",
    "@ngrx/operators": "^19.0.0-beta.0",
    "@ngrx/signals": "^19.0.0-beta.0",
    "firebase": "~11.0.2",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@playwright/test": "^1.49.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^22.10.1",
    "angular-eslint": "^19.0.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "http-server": "^14.1.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "playwright-ng-schematics": "^1.2.1",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.16.0"
  }
}
